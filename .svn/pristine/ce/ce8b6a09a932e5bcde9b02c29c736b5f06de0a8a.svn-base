div.slide{
	position:absolute;
	z-index: 11;
	background-color: #fff;
	cursor: pointer;
	bottom: 50px;
	left: 10px;
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=50)";
	filter: alpha(opacity=50);
	-moz-opacity:0.5;
	-khtml-opacity: 0.5;
	opacity: 0.5;
	height: 33px;
}

div.slide span.next{	
	font-size: 45px;
	font-family:cursive;
	padding-left: 5px;
}

span.next:HOVER {
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=10)";
	filter: alpha(opacity=10);
	-moz-opacity:0.1;
	-khtml-opacity: 0.1;
	opacity: 0.1;
}

div.slide span.prev{	
	font-size: 45px;
	font-family:cursive;
	padding-left: 5px;
}

span.prev:HOVER {
	-ms-filter:"progid:DXImageTransform.Microsoft.Alpha(Opacity=10)";
	filter: alpha(opacity=10);
	-moz-opacity:0.1;
	-khtml-opacity: 0.1;
	opacity: 0.1;
}

